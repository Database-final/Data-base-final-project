<!DOCTYPE html>
<html lang="en">

<head>
	<title> Profile </title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!--===============================================================================================-->
	<link rel="icon" type="image/png" href="/patientProfile/images/icons/spreadsheet-512.png" />
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/patientProfile/vendor/bootstrap/css/bootstrap.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/patientProfile/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/patientProfile/vendor/animate/animate.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/patientProfile/vendor/select2/select2.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/patientProfile/vendor/perfect-scrollbar/perfect-scrollbar.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/patientProfile/css/util.css">
	<link rel="stylesheet" type="text/css" href="/patientProfile/css/main.css">
	<link rel="stylesheet" type="text/css" href="/patientProfile/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="/patientProfile/css/flex-slider.css">
	<!--===============================================================================================-->
</head>

<body>
	<header class="header-area header-sticky">
		<div class="container">
			<div class="row">
				<div class="col-12">
					<nav class="main-nav">
						<!-- ***** Logo Start ***** -->
						<a href="#" class="logo">
                            <p class="radio">Radiology</p>
                        </p>
                            
                        </a>
						<!-- ***** Logo End ***** -->
						<!-- ***** Menu Start ***** -->
						<ul class="nav">
							<li><a href="/patientProfile/views/home.ejs">Home</a></li>
							<li><a href="/patientProfile/views/viewPatient.ejs">SignUp</a></li>
							<li><a href="/patientProfile/views/viewDoc.ejs">Login</a></li>
							<li><a href="/patientProfile/views/viewTech.ejs" class="active">PatientView</a></li>
							<li><button class="btn btn--radius-2 btn--blue" type="submit" style="background-color: #c44cba;
								border-radius: 20px;
								font-weight: bold;
								color: white;
								font-family: Verdana, Geneva, Tahoma, sans-serif;">logout</button></li>

						</ul>
						<a class='menu-trigger'>
							<span>Menu</span>
						</a>
						<!-- ***** Menu End ***** -->
					</nav>
				</div>
			</div>
		</div>
	</header>
	<div class="limiter">
		<div class="container-table100">
			
			<div class="wrap-table100">
				<div class="table100">
					<table>
						<thead>
							<tr class="table100-head">
								<th class="column1">Name</th>
								<th class="column2">SSN</th>
								<th class="column3">ID</th>
								<th class="column4">Sex</th>
								<th class="column5">Birthdate</th>

								<th class="column7">Phone</th>

								<th class="column9">Email</th>
								<!--<th class="column10">Password</th>-->
								<th class="column10">Scans</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td class="column1">
									<%= patient.name %>
								</td>
								<td class="column2">
									<%= patient.ssn %>
								</td>
								<td class="column3">
									<%= patient._id %>
								</td>
								<td class="column4">
									<%= patient.sex %>
								</td>
								<td class="column5">
									<%= patient.birthdate %>
								</td>

								<td class="column7">
									<%= patient.phoneNumber %>
								</td>

								<td class="column9">
									<%= patient.email %>
								</td>
								<td class="column10">
									<ul>
										<% files.map(file => { %>
										  <li>
											<a href="<%= file.url %>"><%= file.name %></a>
										  </li>
										<% }) %>
									</ul>
								</td>
							</tr>
							<div class="button_cont"> <button class="example_b" onclick="btnClicked()" target="_blank"
									rel="nofollow noopener">Book</button>
								<button class="example_b" onclick="btnClicked1()" target="_blank"
									rel="nofollow noopener">Complain</button>
							</div>
						</tbody>
					</table>
				
				</div>
			</div>
		</div>
	</div>
	<style>
		* {
			box-sizing: border-box;
		}

		.column {
			float: left;
			width: 33.33%;
			padding: 5px;
		}

		/* Clearfix (clear floats) */
		.row::after {
			content: "";
			clear: both;
			display: table;
		}
	</style>
	<script>
		function btnClicked() {
			document.location.href = "https://www.google.com/";
		}
		function btnClicked1() {
			document.location.href = "https://www.google.com/";
		}
	</script>

<script src="/patientProfile/js/bootstrap.min.js"></script>
	<script src="/patientProfile/js/jquery.counterup.min.js"></script>
	<script src="/patientProfile/js/jquery-2.1.0.min.js"></script>
	<script src="/patientProfile/js/custom.min.js"></script>
	<script src="/patientProfile/js/imgfix.min.js"></script>
	<script src="/patientProfile/js/scrollreveal.min.js"></script>
	<script src="/patientProfile/js/waypoints.min.js"></script>
	<script src="/patientProfile/js/popper.js"></script>

	<!--===============================================================================================-->
	<script src="/patientProfile/vendor/jquery/jquery-3.2.1.min.js"></script>
	<!--===============================================================================================-->
	<script src="/patientProfile/vendor/bootstrap/js/popper.js"></script>
	<script src="/patientProfile/vendor/bootstrap/js/bootstrap.min.js"></script>
	<!--===============================================================================================-->
	<script src="/patientProfile/vendor/select2/select2.min.js"></script>
	<!--===============================================================================================-->
	<script src="/patientProfile/js/main.js"></script>

</body>

</html>