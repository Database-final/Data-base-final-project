<!DOCTYPE html>
<html lang="en">
<head>
	<title> Doctors </title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
<!--===============================================================================================-->	
	<link rel="icon" type="image/png" href="images/icons/spreadsheet-512.png"/>
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/bootstrap/css/bootstrap.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/select2/select2.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="vendor/perfect-scrollbar/perfect-scrollbar.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="css/util.css">
	<link rel="stylesheet" type="text/css" href="css/main.css">
<!--===============================================================================================-->
</head>
<body>	
	<div class="limiter">
		<div class="container-table100">
			<header class="header-area header-sticky">
				<div class="container">
					<div class="row">
						<div class="col-12">
							<nav class="main-nav">
								<!-- ***** Logo Start ***** -->
								<a href="#" class="logo">
									<img src="images/logo-3.png" alt="Rad"/>
								</a>
								<!-- ***** Logo End ***** -->
								<!-- ***** Menu Start ***** -->
								<ul class="nav">
									
									<li><a href="views/home.ejs" >Home</a></li>
									<li><a href="views/contactUs.ejs">Sign up</a></li>
									<li><a href="views/contactUs.ejs" >Login</a></li>
									
									
								</ul>
								<a class='menu-trigger'>
									<span>Menu</span>
								</a>
								<!-- ***** Menu End ***** -->
							</nav>
						</div>
					</div>
				</div>
			</header> 
			<div class="wrap-table100">
				<div class="table100">
					<% if (doctors.length > 0) { %>	
						<table>
							<thead>
								<tr class="table100-head">
									<th class="column1">Name</th>
									<th class="column2">SSN</th>
									<th class="column3">ID</th>
									<th class="column4">Sex</th>
									<th class="column5">Birthdate</th>
									<th class="column6">Address</th>
									<th class="column7">HoursWorked</th>
									<th class="column8">Phone</th>
									<th class="column9">Email</th>
									<th class="column10">Device</th>
								</tr>
						    </thead>
						    <tbody>
							<% doctors.forEach(doctor => { %>
								<tr>
									<td class="column1"><%= doctor.D_name %></td>
									<td class="column2"><%= doctor.D_ssn %></td>
									<td class="column3"><%= doctor.D_id %></td>
									<td class="column4"><%= doctor.D_sex %></td>
									<td class="column5"><%= doctor.D_bdate %></td>
									<td class="column6"><%= doctor.D_add %></td>
									<td class="column7"><%= doctor.D_hours %></td>
									<td class="column8"><%= doctor.D_phone %></td>
									<td class="column9"><%= doctor.D_email %></td>
									<td class="column10"><%= doctor.D_device %></td>
								</tr>
							<% }) %>
							<div class="button_cont"> 
								<button class="example_b" onclick="btnClicked1()" target="_blank" rel="nofollow noopener">Upload Report</button>
							</div>
						    
					<% } else { %>
							<p class="noDoc" > There are no doctors to show</p>
							
					<%  } %>
						</tbody>
					</table>
					
						
					
				</div>
			</div>
		</div>
	</div>
    <script>
		
		function btnClicked1(){
			document.location.href="https://www.google.com/";
		}
	</script>

	

<!--===============================================================================================-->	
	<script src="vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="vendor/bootstrap/js/popper.js"></script>
	<script src="vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
	<script src="vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
	<script src="js/main.js"></script>

</body>
</html>